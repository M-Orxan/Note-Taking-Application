@model Note


<div>


    <div class="card">

        <div class="card-body">

            <div class="d-flex justify-content-between align-items-center">
                <h4 class="card-title">@Model.Title</h4>

            </div>


            <div>
                <table class="table">
                    <thead>
                        <tr>

                            <th>Title</th>

                            <th>Description</th>
                        </tr>
                    </thead>
                    <tbody>


                        <tr>

                            <td>@Model.Title</td>




                            <td>



                                @Model.Description





                            </td>
                        </tr>

                    </tbody>
                </table>
            </div>

            <a asp-action="Index" style=" margin:10px" class="btn btn-primary">Go Back</a>



        </div>


    </div>


</div>


