@model List<Note>



<ul id="globalSearchList" style="background-color:azure; height:250px; overflow-y:scroll">
    @foreach (Note note in Model)
    {
        <li >
            <a>@note.Title</a>
        </li>
    }
    @if (Model.Count == 0)
    {
        <li style="display:flex;justify-content:center;align-items:center">
            Hec bir netice tapilmadi
        </li>
    }
</ul>



